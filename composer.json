{
    "name": "laravelcollective/remote",
    "description": "Remote SSH access for The Laravel Framework.",
    "license": "MIT",
    "homepage": "http://laravelcollective.com",
    "support": {
        "issues": "https://github.com/LaravelCollective/remote/issues",
        "source": "https://github.com/LaravelCollective/remote"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name": "Tom Shafer",
            "email": "tj@tjshafer.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "illuminate/console": "5.6.*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Collective\\Remote\\RemoteServiceProvider"
            ],
            "aliases": {
                "SSH": "Collective\\Remote\\RemoteFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Collective\\Remote\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
